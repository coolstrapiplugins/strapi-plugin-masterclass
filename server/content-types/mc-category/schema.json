{
  "kind": "collectionType",
  "collectionName": "mc_category",
  "info": {
    "singularName": "mc-category",
    "pluralName": "mc-categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::masterclass.mc-category",
      "mappedBy": "parent_category"
    },
    "parent_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::masterclass.mc-category",
      "inversedBy": "subcategories"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::masterclass.mc-course",
      "mappedBy": "category"
    },
    "featured_courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::masterclass.mc-course",
      "mappedBy": "featured_in"
    },
    "ejercicios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::masterclass.mc-ejercicio",
      "mappedBy": "category"
    },
    "featured_ejercicios": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::masterclass.mc-ejercicio",
      "inversedBy": "featured_in"
    }
  }
}
