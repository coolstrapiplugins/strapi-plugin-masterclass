{
  "kind": "collectionType",
  "collectionName": "mc_course",
  "info": {
    "singularName": "mc-course",
    "pluralName": "mc-courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "lectures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::masterclass.mc-lecture",
      "mappedBy": "course"
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::masterclass.mc-student",
      "inversedBy": "courses"
    },
    "duration": {
      "type": "integer"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    }
  }
}
